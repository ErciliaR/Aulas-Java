package PacoteJava;

public class Skills_and_Mindsets 
{
	public static void main (String args[])
	{
		System.out.println("\nSkills and Mindsets:\n");
		System.out.println("   After watching the videos from Generation and Alura platform, I had"
				+ "\nto be resilient, to communicate my difficulties, to ask for help, to "
				+ "\npay attention to details and to keep focus on the learning process -"
				+ "\ntaking my time to absorb new knowledge.");
	}
}
